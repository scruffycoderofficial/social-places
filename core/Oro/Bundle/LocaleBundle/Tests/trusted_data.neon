# Configuration for https://github.com/oroinc/phpstan-rules/
trusted_data:
    variables:
        Oro\Bundle\LocaleBundle\Entity\Repository\LocalizationRepository:
            getBatchIterator:
                fieldName: true
    safe_methods:
        Oro\Bundle\LocaleBundle\DQL\DQLNameFormatter:
            getFormattedNameDQL: true
    properties:
        Oro\Bundle\LocaleBundle\Autocomplete\EnabledLocalizationsSearchHandler:
            searchEntities:
                idFieldName: true
